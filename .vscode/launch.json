    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["current"]
        },
        {
            "name": "Launch -rod=show,slow=1s,trace",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["current","-rod=show,slow=1s,trace"]
        },
        {
            "name": "Launch auth=google",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["current","-rod=show,slow=1s,trace","-auth=google"]
        }
    ]
}